<div class="container my-5">
  <h2 class="text-center mb-4">User Information</h2>

  <div class="card mb-3">
    <div class="card-body">
      <h5 class="card-title">User: <%= data.rows.username %></h5>
      <p class="card-text">
        <strong>Full Name:</strong> <%= data.rows.fullname %>
      </p>
      <p class="card-text">
        <strong>Address:</strong> <%= data.rows.address %>
      </p>
      <p class="card-text"><strong>Sex:</strong> <%= data.rows.sex %></p>
      <p class="card-text"><strong>Email:</strong> <%= data.rows.email %></p>
    </div>
  </div>
</div>
